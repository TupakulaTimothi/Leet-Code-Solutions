class Solution(object):
    def mergeAlternately(self, word1, word2):
        res = len(word1)+len(word2)
        re=''
        i1=0;i2=0
        while len(re)!=res:
            if i1!=len(word1):
                re=re+word1[i1]
                i1=i1+1
            if i2!=len(word2):
                re = re + word2[i2]
                i2 = i2 + 1
        return re
        """
        :type word1: str
        :type word2: str
        :rtype: str
        """
        
