class Solution(object):
    def productExceptSelf(self, nums):
        answer=[]
        for i in range(len(nums)):
            product=1
            for j in range(len(nums)):
                if i==j:
                    flag=1
                else:
                    product=product*nums[j]
            answer.append(product)
        return answer


print(productExceptSelf([-1,1,0,-3,3]))
